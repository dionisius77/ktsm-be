{
  "name": "monorepo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "prebuild": "rimraf dist && nest start",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:auth-api:dev": "cross-env NODE_ENV=development nest start auth-api --watch",
    "start:debug-auth:dev": "cross-env NODE_ENV=development nest start auth-api --debug --watch",
    "start:auth-api:staging": "cross-env NODE_ENV=staging nest start auth-api --watch",
    "start:debug-auth:staging": "cross-env NODE_ENV=staging nest start auth-api --debug --watch",
    "start:auth-api:prod": "cross-env NODE_ENV=production nest start auth-api --watch",
    "start:debug-auth:prod": "cross-env NODE_ENV=production nest start auth-api --debug --watch",
    "start:message-api:dev": "cross-env NODE_ENV=development nest start message-api --watch",
    "start:message-api:staging": "cross-env NODE_ENV=staging nest start message-api --watch",
    "start:message-api:prod": "cross-env NODE_ENV=production nest start message-api --watch",
    "start:prod": "node dist/apps/monorepo/main",
    "build:auth-api": "nest build auth-api",
    "build:message-api": "nest build message-api",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/monorepo/test/jest-e2e.json"
  },
  "dependencies": {
    "@google-cloud/recaptcha-enterprise": "^4.0.0",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.1.3",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-socket.io": "^10.2.10",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.11",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.10",
    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "eslint-plugin-prettier": "^5.0.1",
    "firebase-admin": "^12.0.0",
    "google-protobuf": "^3.21.2",
    "mathjs": "^11.10.1",
    "multer": "^1.4.5-lts.1",
    "nestjs-redis": "^1.3.3",
    "openai": "^3.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.2",
    "pump": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "stripe": "^13.2.0",
    "typeorm": "^0.3.17",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.3",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "29.5.3",
    "@types/lodash": "^4.14.197",
    "@types/node": "20.4.5",
    "@types/nodemailer": "^6.4.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/pump": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.9.0",
    "i": "^0.3.7",
    "jest": "29.6.2",
    "npm": "^9.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/entities(|/.*)$": "<rootDir>/libs/entities/src/$1",
      "^@app/interceptors(|/.*)$": "<rootDir>/libs/interceptors/src/$1",
      "^@app/helpers(|/.*)$": "<rootDir>/libs/helpers/src/$1"
    }
  }
}
